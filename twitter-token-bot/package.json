{
  "name": "pftweetscanner",
  "version": "1.0.0",
  "description": "PumpFun token scanner with Twitter engagement validation and configurable analysis",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "NODE_ENV=development nodemon src/app.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "logs": "tail -f logs/app.log",
    "health": "node scripts/healthCheck.js",
    "config:validate": "node scripts/validateConfig.js",
    "config:test": "node scripts/validateConfig.js",
    "clean": "rm -rf logs/* temp/*",
    "start:creation": "BOT_MODE=creation node src/app.js",
    "start:migration": "BOT_MODE=migration node src/app.js",
    "start:both": "BOT_MODE=both node src/app.js"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bignumber.js": "^9.1.2",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "node-telegram-bot-api": "^0.64.0",
    "puppeteer": "^21.5.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "user-agents": "^1.0.1293",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "pumpfun",
    "solana",
    "token-monitoring",
    "twitter-validation",
    "telegram-bot",
    "bundle-detection",
    "crypto-scanner",
    "configurable-analysis"
  ],
  "author": "Your Name",
  "license": "MIT"
}